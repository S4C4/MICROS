#include "p16F887.inc"   ; TODO INSERT CONFIG CODE HERE USING CONFIG BITS GENERATOR
 	__CONFIG	_CONFIG1,	_INTRC_OSC_NOCLKOUT & _WDT_OFF & _PWRTE_OFF & _MCLRE_ON & _CP_OFF & _CPD_OFF & _BOR_OFF & _IESO_ON & _FCMEN_ON & _LVP_OFF 
 	__CONFIG	_CONFIG2,	_BOR40V & _WRT_OFF

RES_VECT  CODE    0x0000            ; processor reset vector
    GOTO    START                   ; go to beginning of program

MAIN_PROG CODE                      ; let linker place main program

i equ 0x30
j equ 0x31
k equ 0x32
c equ 0x33

START

    BANKSEL PORTA ;
    CLRF PORTA ;Init PORTA
    BANKSEL ANSEL ;
    CLRF ANSEL ;digital I/O
    CLRF ANSELH
    BANKSEL TRISA ;
    CLRF TRISA
    CLRF TRISB
    CLRF TRISC
    CLRF TRISD
    CLRF TRISE
    BCF STATUS,RP1
    BCF STATUS,RP0
    CLRF PORTA
    CLRF PORTB
    CLRF PORTC
    CLRF PORTD
    CLRF PORTE
    MOVLW D'30'
    MOVWF c
    
    
INICIO
    ESTADO0:
    MOVLW B'11111111'
    MOVWF PORTB
    MOVLW B'00000000'
    MOVWF PORTD
    CALL TIEMPO
    
    DECFSZ c, 1
    GOTO ESTADO0
    MOVLW D'20'
    MOVWF c
    
    ESTADO1:
    MOVLW B'00001110'
    MOVWF PORTB
    MOVLW B'00000001'
    MOVWF PORTD
    CALL TIEMPO
    
    DECFSZ c, 1
    GOTO ESTADO1
    MOVLW D'20'
    MOVWF c
    
    ESTADO2:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'00000011'
    MOVWF PORTD
    CALL TIEMPO
    
    MOVLW B'10011111'
    MOVWF PORTB
    MOVLW B'00000010'
    MOVWF PORTD
    CALL TIEMPO

    DECFSZ c, 1
    GOTO ESTADO2
    MOVLW D'20'
    MOVWF c
    
    ESTADO3:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'00000111'
    MOVWF PORTD
    CALL TIEMPO
    
    MOVLW B'10011111'
    MOVWF PORTB
    MOVLW B'00000100'
    MOVWF PORTD
    CALL TIEMPO

    DECFSZ c, 1
    GOTO ESTADO3
    MOVLW D'20'
    MOVWF c
    
    ESTADO4:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'00001111'
    MOVWF PORTD
    CALL TIEMPO
    
    MOVLW B'10011111'
    MOVWF PORTB
    MOVLW B'00001000'
    MOVWF PORTD
    CALL TIEMPO

    DECFSZ c, 1
    GOTO ESTADO4
    MOVLW D'20'
    MOVWF c
    
    ESTADO5:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'00011111'
    MOVWF PORTD
    CALL TIEMPO
    
    MOVLW B'10011111'
    MOVWF PORTB
    MOVLW B'00010000'
    MOVWF PORTD
    CALL TIEMPO

    DECFSZ c, 1
    GOTO ESTADO5
    MOVLW D'20'
    MOVWF c
    
    ESTADO6:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'00111111'
    MOVWF PORTD
    CALL TIEMPO
    
    MOVLW B'10011111'
    MOVWF PORTB
    MOVLW B'00100000'
    MOVWF PORTD
    CALL TIEMPO

    DECFSZ c, 1
    GOTO ESTADO6
    MOVLW D'20'
    MOVWF c
    
    ESTADO7:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'01111111'
    MOVWF PORTD
    CALL TIEMPO
    
    MOVLW B'10011111'
    MOVWF PORTB
    MOVLW B'01000000'
    MOVWF PORTD
    CALL TIEMPO

    DECFSZ c, 1
    GOTO ESTADO7
    MOVLW D'20'
    MOVWF c
    
    ESTADO8:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'11111111'
    MOVWF PORTD
    CALL TIEMPO
    
    MOVLW B'10011111'
    MOVWF PORTB
    MOVLW B'10000000'
    MOVWF PORTD
    CALL TIEMPO

    MOVLW B'11110001'
    MOVWF PORTB
    MOVLW B'00000001'
    MOVWF PORTD
    CALL TIEMPO
    
    DECFSZ c, 1
    GOTO ESTADO8
    MOVLW D'20'
    MOVWF c
    
    ESTADO9:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'11111110'
    MOVWF PORTD
    CALL TIEMPO

    MOVLW B'11110001'
    MOVWF PORTB
    MOVLW B'00000010'
    MOVWF PORTD
    CALL TIEMPO
    
    DECFSZ c, 1
    GOTO ESTADO9
    MOVLW D'20'
    MOVWF c
    
    ESTADO10:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'11111100'
    MOVWF PORTD
    CALL TIEMPO

    MOVLW B'11110001'
    MOVWF PORTB
    MOVLW B'00000100'
    MOVWF PORTD
    CALL TIEMPO
    
    DECFSZ c, 1
    GOTO ESTADO10
    MOVLW D'20'
    MOVWF c
    
    ESTADO11:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'11111000'
    MOVWF PORTD
    CALL TIEMPO

    MOVLW B'11110001'
    MOVWF PORTB
    MOVLW B'00001000'
    MOVWF PORTD
    CALL TIEMPO
    
    DECFSZ c, 1
    GOTO ESTADO11
    MOVLW D'20'
    MOVWF c
    
    ESTADO12:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'11110000'
    MOVWF PORTD
    CALL TIEMPO

    MOVLW B'11110001'
    MOVWF PORTB
    MOVLW B'00010000'
    MOVWF PORTD
    CALL TIEMPO
    
    DECFSZ c, 1
    GOTO ESTADO12
    MOVLW D'20'
    MOVWF c
    
    ESTADO13:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'11100000'
    MOVWF PORTD
    CALL TIEMPO

    MOVLW B'11110001'
    MOVWF PORTB
    MOVLW B'00100000'
    MOVWF PORTD
    CALL TIEMPO
    
    DECFSZ c, 1
    GOTO ESTADO13
    MOVLW D'20'
    MOVWF c
    
    ESTADO14:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'11000000'
    MOVWF PORTD
    CALL TIEMPO

    MOVLW B'11110001'
    MOVWF PORTB
    MOVLW B'01000000'
    MOVWF PORTD
    CALL TIEMPO
    
    DECFSZ c, 1
    GOTO ESTADO14
    MOVLW D'20'
    MOVWF c
    
    ESTADO15:
    MOVLW B'01101110'
    MOVWF PORTB
    MOVLW B'10000000'
    MOVWF PORTD
    CALL TIEMPO

    MOVLW B'11110001'
    MOVWF PORTB
    MOVLW B'10000000'
    MOVWF PORTD
    CALL TIEMPO
    
    DECFSZ c, 1
    GOTO ESTADO15
    MOVLW D'20'
    MOVWF c
    
    GOTO INICIO
    
TIEMPO
    MOVLW d'1'
    MOVWF i
loopj:
    MOVLW d'2'
    MOVWF j
loopk:
    MOVLW d'250'
    MOVWF k
loopi:
    NOP
    NOP
    DECFSZ k, 1
    GOTO loopi
    DECFSZ j, 1
    GOTO loopk
    DECFSZ i, 1
    GOTO loopj
    RETURN
    
END
